{
  "name": "node-typescript-vscode",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run serve:ts",
    "serve:ts": "npm run nodemon -- --inspect=5858 --config ./nodemon.json --exec {{pwd}}/node_modules/.bin/ts-node ./src/index.ts",
    "serve:js": "node ./build/index.js",
    "build": "npm run compile",
    "compile": "npm run tsc -- -p ./tsconfig.json",
    "clean": "npm run rimraf -- ./build/**/*",
    "lint": "npm run tslint -- -c ./tslint.json -p ./tsconfig.json",
    "lint:fix": "npm run tslint -- -c ./tslint.json -p ./tsconfig.json --fix",
    "test": "exit 0",
    "ci:travis": "npm run lint && npm run test && npm run build",
    "nodemon": "./node_modules/.bin/nodemon",
    "tslint": "./node_modules/.bin/tslint",
    "rimraf": "./node_modules/.bin/rimraf",
    "tsc": "./node_modules/.bin/tsc"
  },
  "devDependencies": {
    "@types/node": "7.0.34",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {}
}
